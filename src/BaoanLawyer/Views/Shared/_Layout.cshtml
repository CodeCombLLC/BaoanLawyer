
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title - @SR["Heilongjiang Baoan Lawyer"]</title>
    <link href='http://fonts.googleapis.com/css?family=Istok+Web:700' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Droid+Sans:700' rel='stylesheet' type='text/css'>
    <link href="~/styles/reset.css" rel="stylesheet" />
    <link href="~/styles/style.css" rel="stylesheet" />
    <link href="~/styles/grid.css" rel="stylesheet" />
    <script src="~/scripts/jquery-1.12.1.min.js"></script>
    <script src="~/scripts/superfish.js"></script>
    <script src="~/scripts/ff-cash.js"></script>
    <script src="~/scripts/easytooltip.js"></script>
    <script src="~/scripts/jquery.cycle.all.min.js"></script>
    <script src="~/scripts/script.js"></script>
    <script>
		$(function(){
			//  Initialize jQuery Cycle Plugin
			$('#slideshow').cycle({
				fx: 'fade',
				speed: 800,
				timeout: 6000,
				prev: '#prev',
				next: '#next',
				pager: '#nav'
			})

			$('#slideshow').hover(
				function(){$(this).find('.banner').stop().animate({right:'0px'}, 300)},
				function(){$(this).find('.banner').stop().animate({right:'-303px'}, 300)}
			)
		})
    </script>
    <!--[if lt IE 8]>
        <div style='clear: both; text-align: center; position: relative;'>
            <a href="http://windows.microsoft.com/en-US/internet-explorer/products/ie/home?ocid=ie6_countdown_bannercode">
                <img src="http://storage.ie6countdown.com/assets/100/images/banners/warning_bar_0000_us.jpg" border="0" height="42" width="820" alt="You are using an outdated browser. For a faster, safer browsing experience, upgrade for free today." />
            </a>
        </div>
    <![endif]-->
    <!--[if lt IE 9]>
        <script type="text/javascript" src="js/html5.js"></script>
        <link rel="stylesheet" type="text/css" href="css/ie.css" media="screen" />
    <![endif]-->
</head>
<body id="page1">
    <div class="block-1">
        <div class="main">
            <!-- header -->
            <header>
                <!-- menu -->
                <nav class="fl-l">
                    <ul class="sf-menu">
                        <li class="active"><a href="#">@SR["HOME"]</a></li>
                        <li>
                            <a href="#">@SR["COMPANY"]</a>
                            <ul>
                                <li><a href="#">@SR["HISTORY"]</a></li>
                                <li><a href="#">@SR["NEWS"]</a></li>
                                <li><a href="#">@SR["RESOURCE"]</a></li>
                            </ul>
                        </li>
                        <li>
                            <a href="#">@SR["SERVICE"]</a>
                            <ul>
                                <li><a href="#">@SR["CASES"]</a></li>
                                <li><a href="#">@SR["LAWYERS"]</a></li>
                                <li><a href="#">@SR["PRICE"]</a></li>
                            </ul>
                        </li>
                        <li><a href="#">@SR["PARTNERS"]</a></li>
                        <li><a href="#">@SR["CONTACTS"]</a></li>
                    </ul>

                    <div class="clear"></div>
                </nav><!-- end menu -->

                <div class="block-2 fl-l">
                    <ul class="list-3">
                        <li><a href="#">Register</a></li>
                        <li><a href="#">Login</a></li>
                        <li><a href="#">FAQ</a></li>
                    </ul>
                </div>
                <div class="clear"></div>
            </header><!-- end header -->
        </div>
    </div>
    @RenderBody()
    <!-- footer -->
    <footer>
        <div class="block-3">
            <div class="container_12">
                <div class="wrapper separator-1">
                    <div class="grid_7">
                        <h2 class="ident-bot-2">Legal Topics</h2>
                        <div class="grid_2 alpha">
                            <ul class="list-2">
                                <li><a href="#">Auto Accidents</a></li>
                                <li><a href="#">Bankruptcy</a></li>
                                <li><a href="#">Business Law</a></li>
                                <li><a href="#">Child Custody</a></li>
                            </ul>
                        </div>

                        <div class="grid_2 suffix_1">
                            <div class="inner-ident-1">
                                <ul class="list-2 marker-2">
                                    <li><a href="#">Child Support</a></li>
                                    <li><a href="#">Civil Rights</a></li>
                                    <li><a href="#">Consumer</a></li>
                                    <li><a href="#">Criminal Law</a></li>
                                </ul>
                            </div>
                        </div>

                        <div class="grid_2 omega">
                            <ul class="list-2 marker-2">
                                <li><a href="#">Discrimination</a></li>
                                <li><a href="#">Divorce</a></li>
                                <li><a href="#">Drunk Driving</a></li>
                                <li><a href="#">Education</a></li>
                            </ul>
                        </div>
                    </div>

                    <div class="grid_5">
                        <div class="inner-ident-2">
                            <div class="block-4">
                                <p class="font-1 ident-bot-2">Our Newsletter</p>
                                <form class="ident-bot-3" id="newsletter" method="get"><input type="text" name="name" /></form>
                                <a class="link-1 fl-l" href="#">Unsubscribe</a>
                                <a href="#" class="button-3 fl-r" onClick="document.getElementById('newsletter').submit()">SUBSCRIBE</a>
                                <div class="clear"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="block-5">
            <div class="main">
                <div class="policy fl-l">
                    <span class="font-2">
                        McAllen
                    </span> <span class="extra-1 inner-ident-3">&copy; 2012 | <a href="#">PRIVACY POLICY</a></span>
                    <div><!--{%FOOTER_LINK} --></div>
                </div>
                <div class="fl-r">
                    <div class="font-3 inner-ident-4 fl-l">Find Us Elsewhere:</div>
                    <ul class="fl-l list-4">
                        <li><a href="#" class="tooltip" title="Twitter"><img src="images/soc-icon-1.png" alt="" /></a></li>
                        <li><a href="#" class="tooltip" title="Facebook"><img src="images/soc-icon-2.png" alt="" /></a></li>
                        <li><a href="#" class="tooltip" title="Google+"><img src="images/soc-icon-3.png" alt="" /></a></li>
                    </ul>
                </div>
                <div class="clear"></div>
            </div>
        </div>
    </footer><!-- end footer -->
</body>
</html>